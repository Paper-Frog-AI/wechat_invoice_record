/* pages/index/listInvoice.wxss */
/* table css */
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}
.td-n {
    width:20%;
    justify-content: center;
    text-align: center;
}
.td-w {
    width:30%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th-n {
  width: 20%;
  justify-content: center;
  color: #999;
  display: flex;
  height: 2rem;
  align-items: center;
}
.th-w {
  width: 30%;
  justify-content: center;
  color: #999;
  display: flex;
  height: 2rem;
  align-items: center;
}

.userCell{
    background-color: snow;
}
.userCellActive{
    background-color: greenyellow;
}
.popup{
  height:2.58823529em;
  min-height:2.58823529em;
  line-height:2.58823529em;
  font-size:12px
}